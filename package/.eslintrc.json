{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "prefer-const": "error",
    "class-methods-use-this": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle": 0
  }
}
